<Window x:Class="EM3.Windows.Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EM3.Windows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        PreviewKeyDown="Window_PreviewKeyDown"
        Title="Help" Height="226" Width="313">
    <Grid Background="#FFEEEAEA">
        <TextBox IsReadOnly="True" PreviewKeyDown="textBox_PreviewKeyDown" ScrollViewer.VerticalScrollBarVisibility="Auto" x:Name="textBox" TextWrapping="Wrap" Text="TextBox" Margin="0,0,0,22" FontSize="13.333"/>
        <Label x:Name="label" Content="Esc - Fechar" HorizontalAlignment="Left" VerticalAlignment="Bottom" FontSize="9.333"/>

    </Grid>
</Window>
